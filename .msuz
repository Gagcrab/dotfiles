# vim: set filetype=sh :

# Check for desktop config
if [[ $(hostname) == "linux-babpoot" ]] && [ -f $HOME/.msuz_babpoot ]; then
    . $HOME/.msuz_babpoot
# Check for Ideapad config
elif [[ $(hostname) == "msuz-flex5-linux.msuz.code" ]] && [ -f $HOME/.msuz_flex5 ]; then
    . $HOME/.msuz_flex5
# Check for *old* laptop config
elif [ -f $HOME/.msuz_x550cc ]; then
    . $HOME/.msuz_x550cc
fi

